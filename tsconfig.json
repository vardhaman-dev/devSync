{
	"compilerOptions": {
	  "target": "ES6",
	  "module": "CommonJS",
	  "outDir": "./out",
	  "rootDir": "./src",
	  "strict": true,
	  "sourceMap": true,
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "skipLibCheck": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", ".vscode-test"]
  }
  